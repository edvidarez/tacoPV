<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.ManageTablesController">
   <children>
      <Pane prefHeight="470.0" prefWidth="592.0">
         <children>
            <Label layoutX="14.0" layoutY="25.0" text="Administrar mesas">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnAddTable" layoutX="14.0" layoutY="416.0" onAction="#btnAddTable" mnemonicParsing="false" prefHeight="40.0" prefWidth="127.0" text="Agregar cuenta">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnCloseTableManagement" layoutX="469.0" layoutY="416.0" onAction="#btnClose" mnemonicParsing="false" prefHeight="40.0" prefWidth="109.0" text="Salir">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <ScrollPane fx:id="scrollPaneManageTables" layoutX="14.0" layoutY="59.0" prefHeight="344.0" prefWidth="564.0">
               <content>
                  <ListView fx:id="listViewManageTable" prefHeight="342.0" prefWidth="562.0" />
               </content>
            </ScrollPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
